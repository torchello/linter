{
    "name": "memio/linter",
    "license": "MIT",
    "type": "library",
    "description": "Memio's linter, a set of constraint that check models for syntax errors",
    "keywords": ["generator", "PHP", "code"],
    "homepage": "http://memio.github.io/memio",
    "authors": [
        {
            "name": "Lo√Øc Faugeron",
            "email": "faugeron.loic@gmail.com",
            "homepage": "http://gnugat.github.io",
            "role": "Developer"
        }
    ],
    "autoload": { "psr-4": {
        "Memio\\Linter\\": "src/Memio/Linter"
    }},
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:torchello/validator.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:torchello/model.git"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "memio/model": "dev-master",
        "memio/validator": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpspec/phpspec": "^7.0"
    }
}
